package ex18.omok;

public class Program1 {

	public static void main(String[] args) {
		for(int x=1; x<=10; x++) {
			for(int y=1; y<=10; y++)
				if(x == 1 && y==1)
					System.out.printf("%c ", '¦£');
				else if(x == 1 && y==10)
					System.out.printf("%c ", '¦¤');
				else if(x == 10 && y==1)
					System.out.printf("%c ", '¦¦');
				else if(x == 10 && y==10)
					System.out.printf("%c ", '¦¥');

				else if(x == 4 && y == 3)
					System.out.printf("%c ", '¡Û');
				else
					System.out.printf("%c ", '¦«');
			
				
			System.out.println();
		}


	}

}
