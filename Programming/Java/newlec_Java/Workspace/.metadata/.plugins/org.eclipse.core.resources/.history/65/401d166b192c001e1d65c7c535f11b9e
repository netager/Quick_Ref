package ex19.array;

import java.util.Random;

public class Program5 {

	public static void main(String[] args) {
		
		Random rand = new Random();
		int[] lotto = new int[6];
		
		lotto[0] = rand
				.nextInt(45);
		
		for(int i=0; i<lotto.length; i++)
			System.out.printf("%d ", lotto[i]);
		
	}

}
