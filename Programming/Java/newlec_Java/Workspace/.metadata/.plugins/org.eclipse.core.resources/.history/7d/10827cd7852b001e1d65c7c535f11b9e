package ex19.array;

public class Program1 {

	public static void main(String[] args) {
		
		int[] nums = {1,2,3,4,5,6,7};
		
		for(int i=0; i<nums.length; i++)
			System.out.printf("%d ", nums[i]);

		System.out.println();
		
		int[] temp = new int[10];
		temp = nums;
		
		for(int i=0; i<temp.length; i++)
			System.out.printf("%d ", temp[i]);
		
	}

}
