package ex19.array;

public class Program1 {

	public static void main(String[] args) {
		
		int[] nums = {1,2,3,4,5,6,7};
		
		for(int i=0; i<nums.length; i++)
			System.out.printf("%d ", nums[i]);

		System.out.println();
		
		int[] temp1 = new int[8];

		for(int i=0; i<nums.length; i++)
			temp1[i] = nums[i];
		
		nums = temp1;

		for(int i=0; i<nums.length; i++)
			System.out.printf("[%d, %d] ", i, temp1[i]);

		
		
		int[] temp = new int[10];

		for(int i=0; i<nums.length; i++)
			temp[i] = nums[i];
		
		nums = temp;
		nums[7] = 8;

		for(int i=0; i<nums.length; i++)
			System.out.printf("(%d, %d) ", i, nums[i]);
		
		
	}

}
