package ex19.array;

public class Program3 {

	public static void main(String[] args) {
		
		int[] nums = {5,2,7,4,6,1,3};
		
		for(int i=0; i<nums.length; i++)
			



			int[] nums1 = {1,2,3};
		int[] nums2 = {4,5,6};
		int[] nums = new int[6];
		
		for(int i=0; i<nums1.length; i++)
			nums[i] = nums1[i];
		for(int i=0; i<nums2.length; i++)
			nums[3+i] = nums2[i];
		
		for(int i=0; i<nums.length; i++)
			System.out.printf("[%d, %d] ", i, nums[i]);
		
	}

}
